<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <changeSet id="002-customer-002" author="jlozano@loquesea.com">
        <createTable tableName="customer">
            <column name="id_customer" type="varchar(100)">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="name" type="varchar(10)">
                <constraints nullable="false"/>
            </column>
            <column name="last_name" type="varchar(10)">
                <constraints nullable="false"/>
            </column>
            <column name="document_type" type="varchar(3)">
                <constraints nullable="false"/>
            </column>
            <column name="document_number" type="varchar(10)">
                <constraints nullable="false"/>
            </column>
            <column name="position" type="varchar(20)">
                <constraints nullable="false"/>
            </column>
            <column name="salary" type="double">
                <constraints nullable="false"/>
            </column>
            <column name="birth_date" type="date">
                <constraints nullable="false"/>
            </column>
            <column name="bonding_date" type="date">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>
